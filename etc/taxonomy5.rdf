        <skos:notation rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral">data_management</skos:notation>
        <skos:notation rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral">epistemology</skos:notation>
